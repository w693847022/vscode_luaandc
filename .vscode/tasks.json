{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: cl.exe build active file",
			"command": "cl.exe",
			"args": [
				"main.cpp",
				"${workspaceFolder}\\sourcefile\\Timer\\*.cpp",
				"/Zi",
				"/EHsc",
				"/I",
				"${fileDirname}\\include",
				"/I",
				"${fileDirname}\\headerfile",
				"${fileDirname}\\lib\\lua\\lua.lib",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}